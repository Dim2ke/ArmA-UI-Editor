<AddIn>
  <info>
    <!-- <name> Displayname of this AddIn -->
    <name>Base</name>
    <!-- <description> contains basic information about this AddIn -->
    <description>Base Add-in containing most base elements available in ArmA 3</description>
    <!-- <author> should contain the authors comma separated-->
    <author>X39</author>
    <!-- Format for <version>: MAJOR '.' MINOR '.' REVISION '-' [ ADDITION ] -->
    <version>1.0.0</version>
    <!-- Format for <updateurl>: VERSION:PACKAGEURLASZIP -->
    <updateurl></updateurl>
  </info>
  <uielements>
    <element>
      <displayname>RscText</displayname>
      <image>/Images/RscText.png</image>
      <class>/Classes/RscText.class</class>
      <xaml>/Xaml/RscText.xaml</xaml>
      <properties>/Properties/RscText.xml</properties>
    </element>
    <element>
      <displayname>RscButton</displayname>
      <image>/Images/RscButton.png</image>
      <class>/Classes/RscButton.class</class>
      <xaml>/Xaml/RscButton.xaml</xaml>
      <properties>/Properties/RscButton.xml</properties>
    </element>
    <element>
      <displayname>IGUIBack</displayname>
      <image>/Images/IGUIBack.png</image>
      <class>/Classes/IGUIBack.class</class>
      <xaml>/Xaml/IGUIBack.xaml</xaml>
      <properties>/Properties/IGUIBack.xml</properties>
    </element>
    <element>
      <displayname>RscCombo</displayname>
      <image>/Images/RscCombo/RscCombo.png</image>
      <class>/Classes/RscCombo.class</class>
      <xaml>/Xaml/RscCombo.xaml</xaml>
      <properties>/Properties/RscCombo.xml</properties>
    </element>
  </uielements>
  <styles>
    <style path="/Styles/DefaultDark.xaml" name="Default Dark"/>
  </styles>
</AddIn>